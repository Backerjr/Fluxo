# OAuth Portal Configuration
# REQUIRED: URL of your OAuth authentication portal
VITE_OAUTH_PORTAL_URL=https://oauth.example.com

# REQUIRED: Your application ID for OAuth
VITE_APP_ID=your-app-id

# Database Configuration
# REQUIRED: MySQL/TiDB connection string
DATABASE_URL=mysql://user:password@host:port/database

# Optional: Node environment (development, production, test)
NODE_ENV=development

# Optional: Server port
PORT=3000
